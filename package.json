{
	"name": "windicss-intellisense",
	"displayName": "WindiCSS IntelliSense",
	"description": "Intelligent WindiCSS tooling for VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:windicss-intellisense.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "windicss-intellisense.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "PUBLISH=true rollup -c",
		"watch": "rollup -cw",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^11.1.1",
		"@rollup/plugin-sucrase": "^3.1.0",
		"@rollup/plugin-typescript": "^8.1.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rollup": "^2.39.0",
		"tslib": "^2.1.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"windicss": "^2.1.4"
	}
}
